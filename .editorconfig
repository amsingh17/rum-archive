# top-most EditorConfig file
root = true

[**]
end_of_line = lf
insert_final_newline = true

[**.js, **.json]
trim_trailing_whitespace = true
indent_style = space
indent_size = 2
quote_type = double

[**.njk]
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

[**.md]
indent_style = space
indent_size = 4
